//returns event for all kind of browser
NSUtil.prototype.preventDefault = function(event)
{
	event = this.getEvent(event);
	if(event.preventDefault) 
	{
	   event.preventDefault();
	}
	else
	{
	   event.returnValue = false;
	}
};

