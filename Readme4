	this.ITEM_DROPPING = "itemDropping";
	this.ITEM_DROPPED = "itemDropped";
	var cancelable = self.util.dispatchEvent(self.__nsList,self.__nsList.ITEM_DROPPING,arrItems,{items:arrItems},true,true);
					if(!cancelable)
					{
						var index = self.__getChildIndex(self.__emptyListItem);
			        	var target = self.util.getTarget(event);
						target = self.util.findParent(target,"ul");
						if(parentID === listContainer.getAttribute("id")) 
						{
							self.__nsList.__moveItem(arrItems,index);
						}
						else
						{
							self.__nsList.__addItem(arrItems,index);
						}
						self.util.preventDefault(event);
						self.__resetList();
						self.util.dispatchEvent(self.__nsList,self.__nsList.ITEM_DROPPED,arrItems,{items:arrItems},true,true);
						return false;
					}
					var setting = {nsTitle:"Demo",template:"templateDemo",minWidth:300,minHeight:300,enableCollapse:true,enableDrag:true, 
					  enableResize:true,enableTitleDblClick:true,styleClass:"panel",titleClass:"panelTitleBar"};
				var divDemo = document.querySelector("#divDemo");
				divDemo.setSetting(setting);
				
				lstDemo.util.addEvent(lstDemo,lstDemo.ITEM_DROPPING,itemDroppingHandler);
						lstDemo.util.addEvent(lstDemo,lstDemo.ITEM_DROPPED,itemDroppedHandler);
						function itemDroppingHandler(event)
				{
					var arrItems = event.detail;
					if(event.cancelable)
					{
						event.preventDefault();
					}
				}
				
				function itemDroppedHandler(event)
				{
					var arrItems = event.detail;
					console.log(arrItems + " dropped");
				}
