var start = new Date().getTime();
	if(!fromRowIndex)
	{
		fromRowIndex = 0;
	}
	var totalRecords = this.__nsGrid.__totalRecords;
	var arrRows = this.__nsGrid.__tblCenterBodyBody.rows;
	var rowLength = arrRows.length;
	var length = this.__nsGrid.__arrFlatHierarchicalSource.length;
	var indexCount = fromIndex;
	var rowCount = fromRowIndex;
	var arrData = this.__nsGrid.__arrFlatHierarchicalSource.slice(fromIndex,toIndex);
	length = arrData.length;
	indexCount = 0;
	var end = new Date().getTime();
	var time = end - start;
	console.log('Execution time: ' + (time / 1000.0));
	for(var count = fromIndex; count < length; count++)
	{
		var item = this.__nsGrid.__arrFlatHierarchicalSource[count];
		if(item[this.__nsGrid.__fieldRowVisible])
		{
			if(rowLength <= rowCount) 
			{
				break;
			}
			var row = arrRows[rowCount];
			row.style.display = "";
			item[this.__nsGrid.__fieldRow] = row;
			this.__updateRow(row,item);
			indexCount++;
			rowCount++;
		}
	}
	end = new Date().getTime();
	time = end - start;
	console.log('Execution time1: ' + (time / 1000.0));
	//for making remaining rows hide
	for(var rowIndex = rowCount;rowIndex < rowLength;rowIndex++)
	{
		var row = arrRows[rowIndex];
		row.style.display = "none";
		row.setAttribute("ns-index",null);
		row.setAttribute("ns-level",null);
		row.setAttribute("ns-parent-index",null);
	}
