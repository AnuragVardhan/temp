this.DRAG_STARTED = "dragStarted";
	this.DRAG_END = "dragEnd";
self.util.dispatchEvent(self.__nsList,self.__nsList.DRAG_STARTED,arrItems,{items:arrItems});
self.util.dispatchEvent(self.__nsList,self.__nsList.DRAG_END,null,null);


lstDemo.util.addEvent(lstDemo,lstDemo.DRAG_STARTED,dragStartHandler);
						lstDemo.util.addEvent(lstDemo,lstDemo.DRAG_END,dragEndHandler);
function dragStartHandler(event)
				{
					var arrItems = event.detail;
					console.log(arrItems + " dragging");
				}
				
				function dragEndHandler(event)
				{
					console.log("drag End Handler");
				}
