nsGroupingGrid.js
Line 34:
this.__groupCollection = new this.util.groupCollection(this.__nsGrid.__dataSource,this.__nsGrid.__childField,this.__nsGrid.__isParentRowField);

NSGroupingGrid.prototype.__updateCellText = function(row,cell,item,colItem,rowIndex,colIndex)
{
	if(cell && item && colItem)
	{
		this.util.removeAllChildren(cell);
		var cellDiv = this.util.createDiv(null);
		var objRowInfo = this.__nsGrid.getRowInfo(row);
		if(objRowInfo)
		{
			 var divText = this.__setBodyCellProperty(row,cell,cellDiv,item,rowIndex,colItem,colIndex,objRowInfo.rowParentIndex,objRowInfo.rowLevel);
			 this.__nsGrid.__highlightDiv(divText,colIndex);
			 cell.appendChild(cellDiv);
		}
	}
};

nsHierarchicalGrid.js
NSHierarchicalGrid.prototype.__updateCellText = function(row,cell,item,colItem,rowIndex,colIndex)
{
	if(cell && item && colItem)
	{
		this.util.removeAllChildren(cell);
		var cellDiv = this.util.createDiv(null);
		var objRowInfo = this.__nsGrid.getRowInfo(row);
		if(objRowInfo)
		{
			 var divText = this.__setBodyCellProperty(row,cell,cellDiv,item,rowIndex,colItem,colIndex,objRowInfo.rowParentIndex,objRowInfo.rowLevel);
			 this.__nsGrid.__highlightDiv(divText,colIndex);
			 cell.appendChild(cellDiv);
		}
	}
};

nsUtil.js
NSUtil.prototype.groupCollection = function(dataSource,childField,parentField)
{
	this.__dataSource = dataSource;
	this.__childField = childField;
	this.__parentField = parentField;
	this.__util = new NSUtil();
	if(!this.__childField)
	{
		this.__childField = "children";
	}
	if(!this.__parentField)
	{
		this.__parentField = "ns-IsParent";
	}
	
	objEmpty[this.__parentField] = true;
