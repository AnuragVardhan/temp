var setting = {container:lstDemo,childNodeType:"LI",enableDragByHandle:true,dragHandlerClass:"handle",enableCloneMode:false,enableDragAfterHold:false,holdTime:500};
		var nsDragDrop = new NSDragDrop(setting);
		nsDragDrop.initialize();
		
		var setting1 = {container:lstDemo1,childNodeType:"LI",enableCloneMode:false};
		var nsDragDrop1 = new NSDragDrop(setting1);
		nsDragDrop1.initialize();
		
		var setting2 = {container:tbl1,childNodeType:"TR",enableDragByHandle:true,dragHandlerClass:"rowhandler",enableCloneMode:false,enableDragAfterHold:false
						};//insertNodeCallback:insertTableNode,moveNodeCallback:moveTableNode,removeNodeCallback:removeTableNode
		var nsDragDrop2 = new NSDragDrop(setting2);
		nsDragDrop2.util.addEvent(tbl1,nsDragDrop2.NODE_DROPPABLE,itemDroppableHandler);
		nsDragDrop2.initialize();
		
		var setting3 = {container:tbl2,childNodeType:"TR",enableDragByHandle:true,dragHandlerClass:"rowhandler",enableCloneMode:true,enableDragAfterHold:false,
						insertNodeCallback:insertTableNode,moveNodeCallback:moveTableNode,removeNodeCallback:removeTableNode};
		var nsDragDrop3 = new NSDragDrop(setting3);
		nsDragDrop3.util.addEvent(tbl2,nsDragDrop3.NODE_DROPPABLE,itemDroppableHandler);
		nsDragDrop3.initialize();
