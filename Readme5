NSSvgShapes.prototype.addFilterIcon = function()
{
	var FILTER_ICON_VIEWPORT = "0 0 14.00 16.80";
	var FILTER_ICON_PATH = "M 1.39999,0L 0,1.40002L 5.60001,8.40002L 5.60001,14L 8.39999," +
						   "16.8L 8.39999,8.40002L 14,1.40002L 12.6," +
						   "0L 1.39999,0 Z M 2.79999,2.80005L 11.2,2.80005L 6.99997,7L 2.79999,2.80005 Z";
	var group = this.__objSVG.createGroup(this.__FILTER_ICON_ID + "group");
	var path = this.__objSVG.createPath(this.__FILTER_ICON_ID + "path",FILTER_ICON_PATH,null,null);
	group.appendChild(path);
	this.__objSVG.addElementInSymbol(this.__FILTER_ICON_ID,FILTER_ICON_VIEWPORT,group);
};
