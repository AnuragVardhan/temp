				   window["extraRowHeaderRenderer"] = function(dataField,colItem,source,rowIndex,colIndex,cell,row)
		{
			var total = 0;
			if(source && source.length > 0)
			{
				var item = {};
				for(var count = 0;count < source.length;count++)
				{
					item = source[count];
					total += item[dataField];
				}
			}
			var htmlText = "" + total;
			return htmlText;
		}
		
